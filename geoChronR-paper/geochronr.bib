	Doi = {10.1029/2009RG000282},
	Isbn = {8755-1209},
	Journal = {Rev. Geophys.},
	Keywords = {solar; climate; variability; 1650 Global Change: Solar variability; 1616 Global Change: Climate variability; 2162 Interplanetary Physics: Solar cycle variations; 3305 Atmospheric Processes: Climate change and variability; 7538 Solar Physics, Astrophysics, and Astronomy: Solar irradiance},
