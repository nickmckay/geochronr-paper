	Journal = {Geophys. Res. Lett.},
	Keywords = {forbush decrease; clouds; cosmic rays; 2104 Interplanetary Physics: Cosmic rays; 2479 Ionosphere: Solar radiation and cosmic ray effects; 3311 Atmospheric Processes: Clouds and aerosols; 0305 Atmospheric Composition and Structure: Aerosols and particles; 1610 Global Change: Atmosphere},
	Month = {02},
