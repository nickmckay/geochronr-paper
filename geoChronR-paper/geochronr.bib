	Doi = {10.1029/2011JD016263},
	Isbn = {0148-0227},
	Journal = {J. Geophys. Res.},
	Keywords = {climate model evaluation; signal-to-noise ratios; tropospheric temperature; 1610 Global Change: Atmosphere (0315, 0325); 1616 Global Change: Climate variability (1635, 3305, 3309, 4215, 4513); 1626 Global Change: Global climate models (3337, 4928); 1641 Global Change: Sea level change (1222, 1225, 4304, 4556)},
	Month = {11},
	Number = {D22},
