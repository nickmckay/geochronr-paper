	Journal = {Journal of Climate},
	Month = {3},
	Number = {5},
	Pages = {1000--1016},
	Title = {Annular Modes in the Extratropical Circulation. Part I: Month-to-Month Variability*},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1175%2F1520-0442%282000%29013%3C1000%3AAMITEC%3E2.0.CO%3B2},
