	Month = {11},
	Number = {1},
	Pages = {71--88},
	Title = {The importance of the geophysical context in statistical evaluations of climate reconstruction procedures},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1007/s10584-007-9276-x},
