	Do = {10.1029/2001JC000841},
	Journal = {J. Geophys. Res.},
	Keywords = {4522 Oceanography: Physical: El Nino; 4572 Oceanography: Physical: Upper ocean processes; 4504 Oceanography: Physical: Air/sea interactions; 4512 Oceanography: Physical: Currents},
	Month = {11},
	Publisher = {American Geophysical Union},
	Title = {Ocean response to the March 1997 Westerly Wind Event},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1029/2001JC000841},
