  Doi = {10.1126/science.1173983},
  Eprint = {http://www.sciencemag.org/content/325/5945/1236.full.pdf},
  Journal = {Science},
  Number = {5945},
  Pages = {1236--1239},
  Title = {{{Recent Warming Reverses Long-Term Arctic Cooling}}},
