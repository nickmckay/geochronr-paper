@article{Kaufman09,
	Abstract = {The temperature history of the first millennium C.E. is sparsely documented, especially in the Arctic. We present a synthesis of decadally resolved proxy temperature records from poleward of 60$\,^{\circ}$N covering the past 2000 years, which indicates that a pervasive cooling in progress 2000 years ago continued through the Middle Ages and into the Little Ice Age. A 2000-year transient climate simulation with the Community Climate System Model shows the same temperature sensitivity to changes in insolation as does our proxy reconstruction, supporting the inference that this long-term trend was caused by the steady orbitally driven reduction in summer insolation. The cooling trend was reversed during the 20th century, with four of the five warmest decades of our 2000-year-long reconstruction occurring between 1950 and 2000.},
	Author = {Kaufman, Darrell S. and Schneider, David P. and McKay, Nicholas P. and Ammann, Caspar M. and Bradley, Raymond S. and Briffa, Keith R. and Miller, Gifford H. and Otto-Bliesner, Bette L. and Overpeck, Jonathan T. and Vinther, Bo M. and Arctic Lakes 2k Project Members},
	Date-Added = {2013-03-06 16:49:50 -0800},
	Date-Modified = {2013-04-16 14:07:42 -0700},
	Doi = {10.1126/science.1173983},
	Eprint = {http://www.sciencemag.org/content/325/5945/1236.full.pdf},
	Journal = {Science},
	Number = {5945},
	Pages = {1236--1239},
