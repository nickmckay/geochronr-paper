	Id = {Huybers2005},
	Isbn = {1476-4687},
	Journal = {Nature},
	Number = {7032},
	Pages = {491--494},
	Title = {Obliquity pacing of the late Pleistocene glacial terminations},
	Ty = {JOUR},
	Url = {https://doi.org/10.1038/nature03401},
	Volume = {434},
	Year = {2005},
	Bdsk-Url-1 = {https://doi.org/10.1038/nature03401}}

