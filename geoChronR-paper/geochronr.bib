	Journal = {J. Clim.},
	Month = {9},
	Number = {18},
	Pages = {4873--4892},
	Title = {Climate Response of the Equatorial Pacific to Global Warming},
	Ty = {JOUR},
	Volume = {22},
	Year = {2009},
