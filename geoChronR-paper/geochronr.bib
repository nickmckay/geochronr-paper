  Date-Modified = {2018-04-14 10:54:42 +0000},
  Journal = {J. Clim.},
  Number = {18},
  Pages = {6801-6843},
  Title = {{Evaluating the Land and Ocean Components of the Global Carbon Cycle in the CMIP5 Earth System Models}},
  Volume = {26},
  Year = {2013},
  Bdsk-Url-1 = {http://dx.doi.org/10.1175/JCLI-D-12-00417.1}}

