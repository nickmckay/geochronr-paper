	Doi = {10.1130/G33510.1},
	Journal = {Geology},
	Title = {Coral record of reduced \nino ~activity in the early 15th to middle 17th centuries},
	Year = {2012},
	Bdsk-Url-1 = {http://geology.gsapubs.org/content/early/2012/10/18/G33510.1.abstract},
	Bdsk-Url-2 = {http://dx.doi.org/10.1130/G33510.1}}

