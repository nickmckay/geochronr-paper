@article{Rodbell99,
	Abstract = {Debris flows have deposited inorganic laminae in an alpine lake that is 75 kilometers east of the Pacific Ocean, in Ecuador. These storm-induced events were dated by radiocarbon, and the age of laminae that are less than 200 years old matches the historic record of El Ni√$\pm$o events. From about 15,000 to about 7000 calendar years before the present, the periodicity of clastic deposition is greater than or equal to 15 years; thereafter, there is a progressive increase in frequency to periodicities of 2 to 8.5 years. This is the modern El Ni√$\pm$o periodicity, which was established about 5000 calendar years before the present. This may reflect the onset of a steeper zonal sea surface temperature gradient, which was driven by enhanced trade winds.},
	Author = {Rodbell, Donald T. and Seltzer, Geoffrey O. and Anderson, David M. and Abbott, Mark B. and Enfield, David B. and Newman, Jeremy H.},
