@article{Jiang:2015aa,
	Author = {Jiang, Hui and Muscheler, Raimund and Bj{\"o}rck, Svante and Seidenkrantz, Marit-Solveig and Olsen, Jesper and Sha, Longbin and Sjolte, Jesper and Eir{\'\i}ksson, J{\'o}n and Ran, Lihua and Knudsen, Karen-Luise and Knudsen, Mads F.},
	Date = {2015/02/20},
	Date-Added = {2017-10-12 22:20:47 +0000},
	Date-Modified = {2017-10-12 22:20:47 +0000},
	Journal = {Geology},
	Journal1 = {Geological Society of America},
	Lp = {206},
	M3 = {10.1130/G36377.1},
	Month = {02},
	N2 = {Mounting evidence from proxy records suggests that variations in solar activity have played a significant role in triggering past climate changes. However, the mechanisms for sun-climate links remain a topic of debate. Here we present a high-resolution summer sea-surface temperature (SST) record covering the past 9300 yr from a site located at the present-day boundary between polar and Atlantic surface-water masses. The record is age constrained via the identification of 15 independently dated tephra markers from terrestrial archives, circumventing marine reservoir age variability problems. Our results indicate a close link between solar activity and SSTs in the northern North Atlantic during the past 4000 yr; they suggest that the climate system in this area is more susceptible to the influence of solar variations during cool periods with less vigorous ocean circulation. Furthermore, the high-resolution SST record indicates that climate in the North Atlantic regions follows solar activity variations on multidecadal to centennial time scales.},
	Number = {3},
	Pages = {203},
	Title = {Solar forcing of Holocene summer sea-surface temperatures in the northern North Atlantic},
	Ty = {JOUR},
	Url = {http://geology.gsapubs.org/content/43/3/203.abstract},
	Volume = {43},
	Year = {2015},
	Bdsk-Url-1 = {http://geology.gsapubs.org/content/43/3/203.abstract}}

