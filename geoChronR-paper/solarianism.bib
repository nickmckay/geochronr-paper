@article{Steinhilber_PNAS2012,
	Annote = {10.1073/pnas.1118965109},
	Author = {Steinhilber, Friedhelm and Abreu, Jose A. and Beer, J{\"u}rg and Brunner, Irene and Christl, Marcus and Fischer, Hubertus and Heikkil{\"a}, Ulla and Kubik, Peter W. and Mann, Mathias and McCracken, Ken G. and Miller, Heinrich and Miyahara, Hiroko and Oerter, Hans and Wilhelms, Frank},
	Date = {2012/04/17},
	Date-Added = {2017-10-12 23:11:06 +0000},
	Date-Modified = {2017-10-12 23:11:20 +0000},
	Journal = {Proceedings of the National Academy of Sciences},
	Journal1 = {Proceedings of the National Academy of Sciences},
	Month = {04},
	Number = {16},
	Pages = {5967--5971},
	Title = {9,400 years of cosmic radiation and solar activity from ice cores and tree rings},
	Ty = {JOUR},
	Url = {http://www.pnas.org/content/109/16/5967.abstract N2 - Understanding the temporal variation of cosmic radiation and solar activity during the Holocene is essential for studies of the solar-terrestrial relationship. Cosmic-ray produced radionuclides, such as 10Be and 14C which are stored in polar ice cores and tree rings, offer the unique opportunity to reconstruct the history of cosmic radiation and solar activity over many millennia. Although records from different archives basically agree, they also show some deviations during certain periods. So far most reconstructions were based on only one single radionuclide record, which makes detection and correction of these deviations impossible. Here we combine different 10Be ice core records from Greenland and Antarctica with the global 14C tree ring record using principal component analysis. This approach is only possible due to a new high-resolution 10Be record from Dronning Maud Land obtained within the European Project for Ice Coring in Antarctica in Antarctica. The new cosmic radiation record enables us to derive total solar irradiance, which is then used as a proxy of solar activity to identify the solar imprint in an Asian climate record. Though generally the agreement between solar forcing and Asian climate is good, there are also periods without any coherence, pointing to other forcings like volcanoes and greenhouse gases and their corresponding feedbacks. The newly derived records have the potential to improve our understanding of the solar dynamics and to quantify the solar influence on climate.},
	Volume = {109},
	Year = {2012},
	Bdsk-Url-1 = {http://www.pnas.org/content/109/16/5967.abstract%20N2%20-%20Understanding%20the%20temporal%20variation%20of%20cosmic%20radiation%20and%20solar%20activity%20during%20the%20Holocene%20is%20essential%20for%20studies%20of%20the%20solar-terrestrial%20relationship.%20Cosmic-ray%20produced%20radionuclides,%20such%20as%2010Be%20and%2014C%20which%20are%20stored%20in%20polar%20ice%20cores%20and%20tree%20rings,%20offer%20the%20unique%20opportunity%20to%20reconstruct%20the%20history%20of%20cosmic%20radiation%20and%20solar%20activity%20over%20many%20millennia.%20Although%20records%20from%20different%20archives%20basically%20agree,%20they%20also%20show%20some%20deviations%20during%20certain%20periods.%20So%20far%20most%20reconstructions%20were%20based%20on%20only%20one%20single%20radionuclide%20record,%20which%20makes%20detection%20and%20correction%20of%20these%20deviations%20impossible.%20Here%20we%20combine%20different%2010Be%20ice%20core%20records%20from%20Greenland%20and%20Antarctica%20with%20the%20global%2014C%20tree%20ring%20record%20using%20principal%20component%20analysis.%20This%20approach%20is%20only%20possible%20due%20to%20a%20new%20high-resolution%2010Be%20record%20from%20Dronning%20Maud%20Land%20obtained%20within%20the%20European%20Project%20for%20Ice%20Coring%20in%20Antarctica%20in%20Antarctica.%20The%20new%20cosmic%20radiation%20record%20enables%20us%20to%20derive%20total%20solar%20irradiance,%20which%20is%20then%20used%20as%20a%20proxy%20of%20solar%20activity%20to%20identify%20the%20solar%20imprint%20in%20an%20Asian%20climate%20record.%20Though%20generally%20the%20agreement%20between%20solar%20forcing%20and%20Asian%20climate%20is%20good,%20there%20are%20also%20periods%20without%20any%20coherence,%20pointing%20to%20other%20forcings%20like%20volcanoes%20and%20greenhouse%20gases%20and%20their%20corresponding%20feedbacks.%20The%20newly%20derived%20records%20have%20the%20potential%20to%20improve%20our%20understanding%20of%20the%20solar%20dynamics%20and%20to%20quantify%20the%20solar%20influence%20on%20climate.}}

