@article{Asmerom_et_al2007,
	Annote = {10.1130/G22865A.1},
	Author = {Asmerom, Yemane and Polyak, Victor and Burns, Stephen and Rassmussen, Jessica},
	Date = {2007/01/01},
	Date-Added = {2017-10-19 17:43:52 +0000},
	Date-Modified = {2017-10-19 17:44:31 +0000},
	Doi = {10.1130/G22865A.1},
	Isbn = {0091-7613},
	Journal = {Geology},
	Month = {01},
	N2 = {Holocene climate change has likely had a profound influence on ecosystems and culture. A link between solar forcing and Holocene climate, such as the Asian monsoon, has been shown for some regions, although no mechanism for this relationship has been suggested. Here we present the first high-resolution complete Holocene climate record for the North American monsoon region of the southwestern United States (southwest) in order to address the nature and causes of Holocene climate change. We show that periods of increased solar radiation correlate with decreased rainfall, the opposite to that observed in the Asian monsoon, and suggest that a solar link to Holocene climate is through changes in the Walker circulation and the Pacific Decadal Oscillation and El Ni{\~n}o--Southern Oscillation systems of the tropical Pacific Ocean. Given the link between increased warming and aridity in the southwest, additional warming due to greenhouse forcing could potentially lead to persistent hyperarid conditions, similar to those seen in our record during periods of high solar activity.},
	Number = {1},
	Pages = {1--4},
	Title = {Solar forcing of Holocene climate: New insights from a speleothem record, southwestern United States},
	Ty = {JOUR},
	Url = {http://dx.doi.org/10.1130/G22865A.1},
	Volume = {35},
	Year = {2007},
	Bdsk-Url-1 = {http://dx.doi.org/10.1130/G22865A.1}}

