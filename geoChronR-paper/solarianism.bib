	Bdsk-Url-2 = {http://dx.doi.org/10.1016/j.quageo.2012.03.015}}

@article{Parnell_QSR2011,
	Abstract = {In this paper we explain the background, workings, and results obtained from three recently developed statistical age-depth models implemented in freely available, general purpose software packages (Bpeat, OxCal and Bchron). These models aim to reconstruct the sedimentation rate in a single core (typically lake, peat or ocean) given a limited number of scientific date estimates (usually radiocarbon) and fixed depths. Most importantly, they provide a suitably qualified estimate of the uncertainty in the age-depth chronology and thus can be used in a variety of applications. We perform a large data-driven study of the three models and discuss their general utility in chronology construction for palaeoenvironmental research. },
